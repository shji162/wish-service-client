{
  "version": 2,
  "builds": [
    {
      "src": "/server/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/client/dist/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/src/main.ts"
    }
  ],
  "buildCommand": "npm run build"
}